<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:jsp="http://java.sun.com/xml/ns/javaee/jsp" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>SCWCDPreparation</display-name>
  <jsp-config>
    <jsp-property-group>
      <display-name>This is JSP Config for Test Application</display-name>
      <url-pattern>*.jsp</url-pattern>
      <el-ignored>false</el-ignored>
      <scripting-invalid>false</scripting-invalid>
      <include-prelude>/header.jsp</include-prelude>
      <include-coda>/footer.jsp</include-coda>
    </jsp-property-group>
  </jsp-config>
  <welcome-file-list>
    <welcome-file>home.jsp</welcome-file>
  </welcome-file-list>
  <security-constraint>
    <web-resource-collection>
      <web-resource-name>restricted methods</web-resource-name>
      <url-pattern>/address.jsp</url-pattern>
      <http-method>GET</http-method>
    </web-resource-collection>
    <user-data-constraint>
      <description>User Transport is Guaranteed</description>
      <transport-guarantee>CONFIDENTIAL</transport-guarantee>
    </user-data-constraint>
  </security-constraint>
  <error-page>
    <exception-type>java.lang.ClassCastException</exception-type>
    <location>/errorPages/classCastException.jsp</location>
  </error-page>
  <error-page>
    <exception-type>java.lang.Throwable</exception-type>
    <location>/errorPages/throwable.jsp</location>
  </error-page>
  <error-page>
    <error-code>302</error-code>
    <location>/errorPages/302.jsp</location>
  </error-page>
  <error-page>
    <error-code>404</error-code>
    <location>/errorPages/404.jsp</location>
  </error-page>
  <error-page>
    <error-code>500</error-code>
    <location>/errorPages/500.jsp</location>
  </error-page>
  <error-page>
    <exception-type>java.lang.Exception</exception-type>
    <location>/errorPages/404.jsp</location>
  </error-page>
  <servlet>
    <servlet-name>CoreServlet</servlet-name>
    <servlet-class>com.test.servlets.core.CoreServlet</servlet-class>
    <init-param>
      <param-name>comeFromWebXml</param-name>
      <param-value>comeFromWebXmlParamValue</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>CoreServlet</servlet-name>
    <url-pattern>/core/*</url-pattern>
  </servlet-mapping>
  <context-param>
    <param-name>contextLevelParam1</param-name>
    <param-value>contextLevelParamValue-1111</param-value>
  </context-param>
  <context-param>
    <param-name>contextLevelParam3</param-name>
    <param-value>contextLevelParamValue-3333</param-value>
  </context-param>
  <context-param>
    <param-name>contextLevelParam2</param-name>
    <param-value>contextLevelParamValue-2222</param-value>
  </context-param>
  <servlet>
    <servlet-name>JspServlet</servlet-name>
    <jsp-file>/coreJspsTest/core.jsp</jsp-file>
    <init-param>
      <param-name>jspParamTestName</param-name>
      <param-value>jspParamTestNameValueInner-22222</param-value>
    </init-param>
    <init-param>
      <param-name>jspParamTestNameOne</param-name>
      <param-value>jspParamTestNameValueOneInner-333333</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>JspServlet</servlet-name>
    <url-pattern>/coreJspsTest/core.jsp</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CoffeeServlet</servlet-name>
    <servlet-class>com.test.servlets.coffee.CoffeeServlet</servlet-class>
    <init-param>
      <param-name>coffeeInitParam1</param-name>
      <param-value>coffeeInitParam1Value</param-value>
    </init-param>
    <init-param>
      <param-name>coffeeInitParam2</param-name>
      <param-value>coffeeInitParam2Value</param-value>
    </init-param>
    <init-param>
      <param-name>coffeeInitParam3</param-name>
      <param-value>coffeeInitParam3Value</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>CoffeeServlet</servlet-name>
    <url-pattern>/coffee/chai</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>com.test.listeners.TestServletContextListener</listener-class>
  </listener>
  <session-config>
    <session-timeout>10</session-timeout>
  </session-config>
  <servlet>
    <servlet-name>ExactMatchServlet</servlet-name>
    <servlet-class>com.test.servlets.urlmapping.ExactMatchServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ExactMatchServlet</servlet-name>
    <url-pattern>/exact/match/servlet.test</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DirectoryMatchServlet</servlet-name>
    <servlet-class>com.test.servlets.urlmapping.DirectoryMatchServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DirectoryMatchServlet</servlet-name>
    <url-pattern>/exact/match/justdirName</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ExtensionMatchServlet</servlet-name>
    <servlet-class>com.test.servlets.urlmapping.ExtensionMatchServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ExtensionMatchServlet</servlet-name>
    <url-pattern>/exact/match/justdirName/*</url-pattern>
  </servlet-mapping>
  <login-config>
    <auth-method>FORM</auth-method>
    <form-login-config>
      <form-login-page>/formLogin.jsp</form-login-page>
      <form-error-page>/formError.jsp</form-error-page>
    </form-login-config>
  </login-config>
  <security-role>
    <role-name>admin</role-name>
  </security-role>
  <security-role>
    <role-name>member</role-name>
  </security-role>
  <security-role>
    <role-name>guest</role-name>
  </security-role>
</web-app>